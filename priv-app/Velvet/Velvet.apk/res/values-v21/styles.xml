<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.SearchNow" parent="@style/Theme.Velvet">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>
    <style name="WallpaperCropperActionBar" parent="@android:style/Widget.DeviceDefault.ActionBar">
        <item name="android:background">#88000000</item>
        <item name="android:displayOptions">showCustom</item>
        <item name="android:contentInsetStart">0.0dip</item>
        <item name="android:contentInsetEnd">0.0dip</item>
        <item name="android:contentInsetLeft">0.0dip</item>
        <item name="android:contentInsetRight">0.0dip</item>
    </style>
    <style name="Theme" parent="@style/BaseWallpaperTheme">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:colorControlActivated">@color/launcher_accent_color</item>
        <item name="android:colorPrimary">@color/launcher_accent_color</item>
        <item name="android:colorAccent">@color/launcher_accent_color</item>
    </style>
    <style name="ActionBarSetWallpaperStyle" parent="@android:style/Widget.DeviceDefault.ActionButton">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">?android:selectableItemBackgroundBorderless</item>
    </style>
    <style name="Theme.Light.CustomOverscroll" parent="@android:style/Theme.DeviceDefault">
        <item name="android:colorEdgeEffect">@color/folder_edge_effect_color</item>
    </style>
    <style name="Theme.Dark.CustomOverscroll" parent="@android:style/Theme.DeviceDefault">
        <item name="android:colorEdgeEffect">@color/workspace_edge_effect_color</item>
    </style>
    <style name="PhotoViewTheme" parent="@style/Theme.Shim">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:actionBarStyle">@style/Holo.ActionBar</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:colorPrimaryDark">@color/photo_status_bar_black</item>
        <item name="android:navigationBarColor">@color/photo_status_bar_black</item>
    </style>
    <style name="AccountsSwitcher.AccountDisplayNameStyle" parent="@style/AccountsSwitcher">
        <item name="android:textStyle">normal</item>
    </style>
    <style name="AccountsSwitcher.SelectedAccountButtonStyle" parent="@android:attr/borderlessButtonStyle">
        <item name="android:background">?android:selectableItemBackgroundBorderless</item>
        <item name="android:layout_width">@dimen/expander_icon_width</item>
        <item name="android:layout_alignParentEnd">true</item>
    </style>
    <style name="MarkyNotificationTitle" parent="@android:style/TextAppearance.Material.Notification.Title" />
    <style name="MarkyNotificationText" parent="@android:style/TextAppearance.Material.Notification" />
    <style name="E300BulletSpinner" parent="@android:style/Widget.Material.ProgressBar.Small">
        <item name="android:layout_gravity">top</item>
        <item name="android:layout_width">@dimen/e300_enroll_image_height</item>
        <item name="android:layout_height">@dimen/e300_enroll_image_height</item>
        <item name="android:indeterminateTint">@color/e300_primary_action_text</item>
        <item name="android:indeterminateTintMode">src_in</item>
    </style>
    <style name="qp_fab">
        <item name="android:layout_gravity">bottom|center|right</item>
        <item name="android:layout_width">@dimen/qp_fab_size</item>
        <item name="android:layout_height">@dimen/qp_fab_size</item>
        <item name="android:layout_marginRight">@dimen/qp_fab_margin</item>
        <item name="android:layout_marginBottom">@dimen/qp_fab_margin</item>
        <item name="android:foreground">@drawable/qp_fab_foreground</item>
        <item name="android:elevation">@dimen/qp_fab_elevation</item>
    </style>
    <style name="setup_wizard_theme_material" parent="@style/Theme.Shim.NoActionBar">
        <item name="android:textColorSecondary">@color/setup_wizard_color_secondary_dark</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="android:colorAccent">@color/setup_wizard_color_accent_dark</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:indeterminateTint">@color/setup_wizard_progress_bar_color_dark</item>
        <item name="setup_wizard_divider_color">@color/setup_wizard_divider_color_dark</item>
        <item name="setup_wizard_link_color">@color/setup_wizard_link_color_dark</item>
    </style>
    <style name="setup_wizard_theme_material.light" parent="@style/Theme.Shim.Light.NoActionBar">
        <item name="android:textColorSecondary">@color/setup_wizard_color_secondary_light</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="android:colorAccent">@color/setup_wizard_color_accent_light</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:indeterminateTint">@color/setup_wizard_progress_bar_color_light</item>
        <item name="setup_wizard_divider_color">@color/setup_wizard_divider_color_light</item>
        <item name="setup_wizard_link_color">@color/setup_wizard_link_color_light</item>
    </style>
    <style name="setup_wizard_display1" parent="@android:style/TextAppearance.Material.Display1" />
    <style name="setup_wizard_text_medium" parent="@android:style/TextAppearance.Material.Medium" />
    <style name="setup_wizard_radiobutton">
        <item name="android:textAppearance">@style/setup_wizard_text_medium</item>
        <item name="android:textSize">@dimen/setup_wizard_description_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textColorLink">?setup_wizard_link_color</item>
        <item name="android:layout_marginTop">@dimen/setup_wizard_content_paragraph_margin_top</item>
        <item name="android:paddingStart">@dimen/setup_wizard_radio_button_padding</item>
    </style>
    <style name="setup_wizard_bottom_scroll_view">
        <item name="android:elevation">@dimen/setup_wizard_card_elevation</item>
    </style>
    <style name="setup_wizard_title_area">
        <item name="android:elevation">@dimen/setup_wizard_title_area_elevation</item>
    </style>
    <style name="setup_wizard_button_borderless" parent="@android:style/Widget.Material.Button.Borderless" />
    <style name="GsaToolBar" parent="@android:style/Widget.Material.Toolbar">
        <item name="android:background">@color/qp_blue</item>
        <item name="android:titleTextAppearance">@style/GsaActionBarTitleText</item>
        <item name="android:subtitleTextAppearance">@style/GsaActionBarSubtitleText</item>
    </style>
    <style name="Theme.OptIn.NoFullBleed" parent="@style/Theme.OptIn">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:statusBarColor">@color/opt_in_status_bar_color</item>
    </style>
    <style name="Theme.E300" parent="@style/setup_wizard_theme_material.light">
        <item name="android:statusBarColor">@color/e300_primary_color_dark</item>
    </style>
    <style name="Theme.Gsa" parent="@style/Theme.Shim.Light">
        <item name="android:textColorHighlight">@color/qp_highlight</item>
        <item name="android:actionBarStyle">@style/Gsa</item>
        <item name="android:alertDialogTheme">@style/Theme.Gsa.Dialog</item>
        <item name="android:textColorHighlightInverse">@color/qp_highlight</item>
        <item name="android:colorControlActivated">@color/qp_blue</item>
        <item name="android:colorPrimary">@color/qp_blue</item>
        <item name="android:colorPrimaryDark">@android:color/black</item>
        <item name="android:colorAccent">@color/qp_blue</item>
        <item name="qpActionIconAlpha">0.38</item>
        <item name="qpActionIconTint">@android:color/black</item>
        <item name="qpH1TextSize">@dimen/qp_h1_text_size</item>
        <item name="qpH1FontFamily">sans-serif-light</item>
        <item name="vogPromptColor">@color/qp_text_b2</item>
        <item name="vogPromptTextStyle">2</item>
    </style>
    <style name="Theme.Gsa.ActionBar" parent="@style/Theme.Gsa">
        <item name="android:actionBarStyle">@style/GsaActionBar</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_arrow_back</item>
        <item name="android:actionMenuTextColor">@color/qp_text_w1</item>
        <item name="android:actionBarTheme">@style/ThemeOverlay.Gsa.ActionBar</item>
        <item name="android:colorPrimary">@color/qp_blue</item>
        <item name="android:colorPrimaryDark">@color/qp_blue_dark</item>
    </style>
    <style name="Theme.Gsa.ActionBar.Drawer" parent="@style/Theme.Gsa.ActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:toolbarStyle">@style/GsaToolBar</item>
    </style>
    <style name="Theme.Gsa.Dialog" parent="@style/Theme.Shim.Light.Dialog">
        <item name="android:colorAccent">@color/qp_blue</item>
    </style>
    <style name="ThemeOverlay.Gsa.ActionBar" parent="@style/ThemeOverlay.Shim.ActionBar">
        <item name="android:colorControlHighlight">@color/qp_ripple_dark</item>
    </style>
    <style name="Theme.Velvet" parent="@style/Theme.Gsa">
        <item name="android:windowBackground">@color/activity_background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowSoftInputMode">adjustPan|stateUnchanged</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@color/status_bar_background</item>
    </style>
    <style name="Theme.SecondScreenReader" parent="@style/Theme.Gsa.ActionBar.Drawer" />
    <style name="Theme.VoiceIme" parent="@style/Theme.Shim.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">@color/ime_text</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:textColorAlertDialogListItem">@color/ime_text</item>
        <item name="android:alertDialogTheme">@style/Theme.VoiceIme.ImeLangDialog</item>
    </style>
    <style name="Theme_Holo_Light_Dialog_Alert" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowMinWidthMajor">@android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@android:dimen/dialog_min_width_minor</item>
    </style>
    <style name="Theme.Shim" parent="@android:style/Theme.Material" />
    <style name="Theme.Shim.NoActionBar" parent="@android:style/Theme.Material.NoActionBar" />
    <style name="Theme.Shim.Dialog.NoActionBar" parent="@android:style/Theme.Material.Dialog.NoActionBar" />
    <style name="Theme.Shim.Dialog.NoActionBar.MinWidth" parent="@android:style/Theme.Material.Dialog.NoActionBar.MinWidth" />
    <style name="Theme.Shim.Light" parent="@android:style/Theme.Material.Light" />
    <style name="Theme.Shim.Light.NoActionBar" parent="@android:style/Theme.Material.Light.NoActionBar" />
    <style name="Theme.Shim.Light.Dialog" parent="@android:style/Theme.Material.Light.Dialog" />
    <style name="Widget.Shim.Light.ActionBar" parent="@android:style/Widget.Material.Light.ActionBar" />
    <style name="TextAppearance.Shim.Widget.ActionBar.Title" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title" />
    <style name="TextAppearance.Shim.Widget.ActionBar.Subtitle" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Subtitle" />
    <style name="AccountsSwitcher.BackgroundStyle" parent="@style/AccountsSwitcher">
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="ThemeOverlay.Shim.ActionBar" parent="@android:style/ThemeOverlay.Material.ActionBar" />
    <style name="Theme.VoiceIme_v2" parent="@style/Theme.VoiceIme">
        <item name="android:textColor">@color/ime_hint_text</item>
        <item name="android:textColorAlertDialogListItem">@color/ime_hint_text</item>
    </style>
</resources>
