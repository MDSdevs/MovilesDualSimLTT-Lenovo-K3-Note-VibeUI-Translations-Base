<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="no_title" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:windowBackground">@color/appcolor</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowTitleBackgroundStyle">@style/uititlebackground</item>
        <item name="android:windowFullscreen">false</item>
    </style>
    <style name="com_lenovo_lsf_ui.setup_no_title_transparent" parent="@android:style/Theme.DeviceDefault.Light.NoActionBar.TranslucentDecor">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
</resources>
